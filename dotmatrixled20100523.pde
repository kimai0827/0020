/*----------------------------------------
----------------------------------------*/
void setup()
{
 uint8_t i;

 
//  pinMode(10, OUTPUT);      // sets the digital pin as output
//  digitalWrite(10, LOW);
  
 for(i=0; i<8; i++){
   myMatrix.bitBlt16(i, data1[i]);
 }
  
  myMatrix.setOutputEnable(true);
  
//  Serial.begin(9600);
}

/*----------------------------------------
----------------------------------------*/
void loop()
{
  int i;
  
  myMatrix.hsync();            //600us 
  delayMicroseconds(200);    //800us
  cycle++;
  
  if(cycle == 200*1000){
    cycle=1;
    ledpos++;
    if(ledpos==2){
      ledpos=0;
    }
  }

   
   if(cycle == 1){ 
     for(i=0; i<8; i++){
        switch(ledpos){
          case  0:
             myMatrix.bitBlt16(i, data1[i]);
          break;
          case  1:
             myMatrix.bitBlt16(i, data2[i]);
          break;
          case  2:
             myMatrix.bitBlt16(i, data3[i]);
          break;
          case  3:
             myMatrix.bitBlt16(i, data4[i]);
          break;
          case  4:
             myMatrix.bitBlt16(i, data5[i]);
          break;
          case  5:
             myMatrix.bitBlt16(i, data6[i]);
          break;
          case  6:
             myMatrix.bitBlt16(i, data7[i]);
          break;
        }
      }
   }
}

